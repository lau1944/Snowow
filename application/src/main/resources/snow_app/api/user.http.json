{
  "name": "User",
  "version": 1.0,
  "path": "/user",
  "paths": [
    {
      "name": "getUserInfo",
      "path": "/info",
      "method": "GET",
      "action": {
        "name": "redirect",
        "url": "https://http.cat",
        "params": {
          "status": "@{params.status}"
        }
      },
      "response": {
        "status": 200,
        "type": "application/json",
        "data": {
          "type": "User",
          "value": {
            "name": "@{configs.env.status}",
            "age": 22,
            "school": [
              {
                "name": "@{params.school}",
                "age": 4
              },
              {
                "name": "Cornell University",
                "age": 2
              }
            ]
          }
        }
      }
    }
  ]
}